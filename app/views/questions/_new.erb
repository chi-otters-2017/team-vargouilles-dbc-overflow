<div id="new-question-form" class="container">
    <h1>New Question!</h1>
  <form method="post" action="/questions">
      <label for="question-title">Title</label>
      <input id="question-title"  name="question[title]" type="text">
      <br>
      <textarea id="question-content" name="question[content]" rows="10" cols="50"></textarea>
      <input type="submit" value="submit">
  </form>
</div>
